} else if (error.name == \"UserNotFoundException\") {\r\n      swal(\" \", \"דוא\\\"ל זה אינו רשום במערכת, יש להרשם או לפנות למנהלי האתר\", \"error\", {\r\n        button: \"אישור\",\r\n      });\r\n    } else\r\n      swal(\" \", \"אחד מהפרטים אינם נכונים\", \"error\", {\r\n        button: \"אישור\",\r\n      });\r\n  }\r\n}\r\n\r\n//The SignIn component.\r\nexport default function SignIn() {\r\n  const classes = useStyles();\r\n  const [loading, setLoading] = useState(false);\r\n  history = useHistory();\r\n\r\n  //Get values from user and run the authentication function.\r\n  function doSomething(e) {\r\n    username = document.getElementById(\"email\").value;\r\n    password = document.getElementById(\"password\").value;\r\n    signIn();\r\n    e.preventDefault();\r\n  }\r\n\r\n  //REACT Component with styling for spesific tags.\r\n  return (\r\n    <Container component=\"main\" maxWidth=\"xs\" id=\"allForm\">\r\n      <CssBaseline />\r\n      <div className={classes.paper}>\r\n        <Avatar style={{\r\n          color: \"#ffffff\",\r\n        }} className={classes.avatar}>\r\n          <LockOutlinedIcon />\r\n        </Avatar>\r\n        <Typography component=\"h1\" variant=\"h5\" style={{\r\n          color: \"#132c33\",\r\n        }}>\r\n          ברוכים הבאים למרחב החינוכי השלם\r\n        </Typography>\r\n        <form className={classes.form} onSubmit={doSomething} noValidate>\r\n          <TextField\r\n            className={classes.textField}\r\n            variant=\"outlined\"\r\n            margin=\"normal\"\r\n            required\r\n            fullWidth\r\n            id=\"email\"\r\n            label=\"כתובת מייל\"\r\n            name=\"email\"\r\n            autoComplete=\"email\"\r\n            autoFocus\r\n          />\r\n          <TextField\r\n            className={classes.textField}\r\n            variant=\"outlined\"\r\n            margin=\"normal\"\r\n            required\r\n            fullWidth\r\n            name=\"password\"\r\n      